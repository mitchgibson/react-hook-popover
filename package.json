{
  "name": "thesmitchuation/react-hook-popover",
  "version": "0.0.1",
  "description": "A react hook for facilitating popover functionality",
  "source": "src/index.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "./dist/index.esm.js",
    "./dist/index.cjs.js",
    "./dist/main.css"
  ],
  "scripts": {
    "build-storybook": "build-storybook",
    "bundle": "rollup -c",
    "bundle:pre": "npm run test:silent && npm run clean",
    "build": "npm run bundle:pre && npm run bundle",
    "clean": "rimraf dist && rimraf docs",
    "docs": "npx typedoc src/index.ts",
    "test": "npm run test:lint && npm run test:unit",
    "component:new": "hygen component with-prompt",
    "hook:new": "hygen hook with-prompt",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,js,jsx}\"",
    "format:check": "prettier --list-different \"src/**/*.{ts,tsx,json,js,jsx}\"",
    "start:dev": "npm ci && npm run storybook",
    "storybook": "start-storybook -p 6006 -s ./src/assets -c .storybook",
    "test:silent": "npm run test:lint:silent && npm run test:unit:silent",
    "test:unit": "jest --maxWorkers=2",
    "test:unit:silent": "jest --maxWorkers=2 --silent --no-coverage --bail",
    "test:lint": "eslint ./src --ext .ts,.tsx",
    "test:lint:silent": "eslint ./src/index.ts --ext .ts,.tsx --quiet",
    "publish:finish": "npm publish && git push --tags",
    "publish:patch": "npm run publish:pre && npm version patch && npm run publish:finish",
    "publish:minor": "npm run publish:pre && npm version minor && npm run publish:finish",
    "publish:major": "npm run publish:pre && npm version major && npm run publish:finish",
    "publish:pre": "npm run build && npm run docs && git add docs/ && git commit -m \"docs: update docs\""
  },
  "repository": {
    "type": "git",
    "url": "git@git.intouchinsight.io:experiments/intouch-web-components.git"
  },
  "author": "Mitch Gibson",
  "license": "ISC",
